package cvm.protocol;

struct GpsData
{
    Point location;
    double speed;    // m/s
    double azimuth;  // degree
    int32_t hour;
    int32_t minute;
    int32_t second;
    int32_t year;
    int32_t month;
    int32_t day;
    int8_t quality;
    int8_t status;
    int8_t satelliteNumber;
}
